a = 7
b = -4
c = 3
print(a, b, c)

a = 7
b = -4
c = 3
print(a, b, c, sep="\n")

s1 = "Hello"
s2 = "Balakirev"
print(s1, s2)

# ввод данных
s1, s2 = map(str.strip, input().split())
# здесь продолжите программу
print(f"Word 1: {s1} | Word 2: {s2}")

a, b = map(int, input().split())
print(a ** b)

a, b = map(float, input().split())
print(a + b)

a, b = map(int, input().split())
print(a + b + a*2 + b*4)
