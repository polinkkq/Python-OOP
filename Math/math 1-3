# ввод целого числа
d = int(input())
# здесь продолжите программу
print(abs(d))

# ввод целого числа
d1, d2, d3, d4, d5 = map(int, input().split())
# здесь продолжите программу
print(min(d1, d2, d3, d4, d5))

# ввод целого числа
d1, d2, d3, d4, d5 = map(int, input().split())
# здесь продолжите программу
print(max(d1, d2, d3, d4, d5))
